{
  "name": "student-space",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress": "cypress open",
    "cypress:e2e": "cypress run --e2e",
    "pretty:all": "prettier --check \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\" || prettier --write \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "pretty:staged": "pretty-quick --staged --pattern \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "pretty:quick": "pretty-quick --pattern \"{config,src,stories}/**/*{.js,.jsx,.ts,.tsx}\"",
    "types:check": "tsc --pretty --noEmit --declaration",
    "types:check:watch": "npm run types:check -- --watch",
    "types:declaration": "tspc --project tsconfig.prod.json"
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@nextui-org/react": "^2.3.6",
    "@nextui-org/shared-icons": "^2.0.7",
    "@nextui-org/system": "^2.1.2",
    "@nextui-org/theme": "^2.2.3",
    "@popperjs/core": "^2.11.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@uiw/react-color-circle": "^2.3.0",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.9.0",
    "framer-motion": "^11.1.9",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "postcss": "^8.4.38",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.51.4",
    "react-quill": "^2.0.0",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0",
    "sass": "^1.76.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.11",
    "vite-plugin-pwa": "^0.20.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-docs": "^8.1.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/addon-onboarding": "^8.1.1",
    "@storybook/addon-themes": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/react": "^8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.8.1",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.1.1"
  }
}
